{
  "name": "kosov/yii2-fns-check",
  "description": "Интеграция PHP клиента API ФНС для работы с онлайн-чеками с фреймворком Yii",
  "type": "yii2-extension",
  "license": "MIT",
  "keywords": [
    "yii2",
    "fns",
    "ФНС",
    "api",
    "онлайн-чеки",
    "проверка чеков"
  ],
  "authors": [
    {
      "name": "kosov",
      "email": "akosov@yandex.ru"
    }
  ],
  "require": {
    "ext-json": "*",
    "yiisoft/yii2": "~2.0.0",
    "kosov/fns-check": "^1.1"
  },
  "provide": {
    "php-http/client-implementation": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kosov\\yii\\fnscheck\\": "src"
    }
  }
}
